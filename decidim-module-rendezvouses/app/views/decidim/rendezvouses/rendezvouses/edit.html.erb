<main class="wrapper">
  <% provide :meta_title, "EVENTO" %>

  <br>
  <div class="row">
    <div class="columns large-12 medium-centered">
      <%= decidim_form_for(@form, html: {class: "form edit_rendezvous"}) do |f| %>
        <%= render partial: 'form', object: f, locals: {title: t('.title')} %>

        <div class="button--double form-general-submit">
          <%= f.submit t(".update") %>
        </div>
      <% end %>
    </div>
  </div>
</main>